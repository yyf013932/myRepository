

# -*- coding:utf-8 -*-
class solution:
    def solve(self,A):
        length=len(A)
        if length==0 or length==1:
            return A
        #交换基准数与列表最后一个数的位置
        k=A[int(length/2)]
        A[int(length/2)]=A[length-1]
        A[length-1]=k
        i=0
        j=length-2
        while i<j:
            print("in")
            while A[i]<A[length-1]:
                i=i+1
            while A[j]>A[length-1]:
                j=j-1
            #交换大数小数的位置
            if i<j:
                k=A[i]
                A[i]=A[j]
                A[j]=k
        k=A[i]
        A[i]=A[length-1]
        A[length-1]=k
        print(A)    
        return 0
    
so=solution()
print (so.solve([2,5,4,6,1]))
        
        