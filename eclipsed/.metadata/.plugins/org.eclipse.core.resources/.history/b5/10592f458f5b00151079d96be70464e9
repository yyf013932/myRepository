

# -*- coding:utf-8 -*-
class solution:
    def solve(self,A):
        if len(A)==1 or len(A)==0:
            return A
        length=len(A)
        k=A[int(length/2)]
        A[int(length/2)]=A[length-1]
        A[length-1]=k
        i=0
        j=length-1
        while i<j:
            while A[i]<=A[length-1]:
                i=i+1
            while A[j]>=A[length-1]:
                j=j-1
            k=A[i]
            A[i]=A[j]
            A[j]=k
        k=A[i]
        A[i]=A[length-1]
        A[length-1]=A[i]
        B=[]
        C=[]
        result=[]
        for k in range(i-1):
            B.append(A[k])
        for k in range(i+1,length-1):
            C.append(A[k])
        result=result+self.solve(B)
        print(B)
        result.append(A[i])
        print(result)
        result=result+self.solve(C)
        return result
    
so=solution()
print (so.solve([2,5,4,6]))
        
        